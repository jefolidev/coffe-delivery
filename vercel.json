{
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "dist/**/*",
      "use": "@vercel/static"
    }
  ],
  "headers": [
    {
      "source": "/coffee-delivery/assets/*.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/coffee-delivery/assets/*",
      "destination": "/dist/assets/"
    }
  ]
}
